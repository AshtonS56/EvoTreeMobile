{
  "expo": {
    "name": "EvoTree",
    "slug": "evotree",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./public/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.evotree.mobile",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "EvoTree accesses your photo library for tree image export.",
        "NSPhotoLibraryAddUsageDescription": "EvoTree saves your phylogenetic tree image to your photo library.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.evotree.mobile",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./public/favicon.ico"
    },
    "plugins": [
      "expo-media-library",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "3d1c27fb-1365-4be5-be23-a4b9d56422c6"
      }
    },
    "owner": "ashtonsenti"
  }
}
